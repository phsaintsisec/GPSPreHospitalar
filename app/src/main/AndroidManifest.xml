<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.td1026.gpsemergenciav20">



    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Activity_Main" android:theme="@style/AppTheme.NoActionBar" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <service android:name=".Activity.Activity_ServicePrincipal" android:exported="true" android:enabled="true"/>
        <activity android:name=".Activity.Activity_ConfiguracoesGerais" android:theme="@style/AppTheme.NoActionBar" android:label="@string/title_activity_activity__configuracoes_gerais"/>
        <activity android:name=".Activity.Activity_DescricaoHospital" android:theme="@style/AppTheme.NoActionBar" android:label="@string/title_activity_activity__descricao_hospital"/>
        <activity android:name=".Activity.Activity_Historico" android:theme="@style/AppTheme.NoActionBar" android:label="@string/title_activity_activity__historico"/>
        <activity android:name=".Activity.Activity_Hospitais" android:theme="@style/AppTheme.NoActionBar" android:label="@string/title_activity_activity__hospitais"/>

    </application>
</manifest>